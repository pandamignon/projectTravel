<script>
    $("#form1").submit(function (event) {
        var formData = {
            'user': $("#user").val(),
            'pwd': $("#pwd").val(),

        };
        //console.log(JSON.stringify(formData));
        $.ajax({
            type: "POST",
            url: "/login/submit",
            data: formData,
            dataType: 'json',
            encoder: true
        }).done(function (data) {
            //alert("data -> "+JSON.stringify(data));
            window.location.href = "/";
        });
        event.preventDefault();
    })
</script>

