<script>
		$("#form1").submit(function(event){
			var formData = {
                'user':$("#user").val(),
                'pwd': $("#pwd").val(),
				'name':$("#name").val(),
				'lname':$("#lname").val(),
				'email':$("#email").val(),
				'tel':$("#tel").val()
            };
            console.log(JSON.stringify(formData));
			$.ajax({
				type:"POST",
				url:"/register/submit",
				data:formData,
				dataType:'json',
				encoder:true
			}).done(function(data){
                //alert("data -> "+JSON.stringify(data));
                window.location.href = "/login";
			});
			event.preventDefault();
		})
	</script>


